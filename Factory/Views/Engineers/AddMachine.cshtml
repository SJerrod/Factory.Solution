@{
  Layout = "_Layout";
}

@model Factory.Models.Engineer

<h2>Add a Machine license</h2>

<h4>Add a Machine that @Html.DisplayFor(model => model.Name) has been trained to operate:</h4>

@using (Html.BeginForm())
{
  @Html.HiddenFor(model => model.EngineerId)
  @Html.Label("Select Machine")
  @Html.DropDownList("MachineId")

  <input type="submit" value="Save" />
}