<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Factory</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link rel="stylesheet" href="~/css/styles.css">
  </head>
  <body>
    <nav class="navbar navbar-inverse">
      <div class="container-fluid">
        <ul class="nav navbar-nav">
        <button type="button" class="btn btn-dark">
            <span class="glyphicon glyphicon-home" aria-hidden="true"></span><li>@Html.ActionLink("Home", "Index", "Home")</li>
          </button>
          
          <button type="button" class="btn btn-dark">
            <span class="glyphicon glyphicon-wrench" aria-hidden="true"></span><li>@Html.ActionLink("See all Engineers", "Index", "Engineers")</li>
          </button>
          
          <button type="button" class="btn btn-dark">
            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span><li>@Html.ActionLink("See all Machines", "Index", "Machines")</li>
          </button>
        </ul>
      </div>
    </nav>
    @RenderBody()
  </body>
</html>